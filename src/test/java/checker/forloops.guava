epicarp {
    int result = 0;

    for (int i = 0; i < 10; i++) {
        result = result + i;
    }

    result = 0;

    int i = 0;
    for (i; i < 10; i++) {
        result = result + i;
    }

    result = 0;

    for (int j = 0; j < 10; j = j + 1) {
        result = result + j;
    }

    result = 0;

    for (int j = 0; j >= 0; j--) {
        result = result + j;
    }
}